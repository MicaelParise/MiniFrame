{
  "name": "micaelparise/miniframe",
  "description": "MiniFrame - mini framework em PHP 8.3 seguindo PSR-1, PSR-4 e PSR-12",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Micael Parise",
      "email": "micaelparise@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "scripts": {
    "lint": "php-cs-fixer fix --dry-run --diff",
    "analyse": "phpstan analyse src"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
